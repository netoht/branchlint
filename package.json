{
  "name": "branchlint2",
  "version": "0.0.2",
  "description": "A branch name linter",
  "main": "src/cli.js",
  "type": "module",
  "bin": {
    "branchlint": "bin/branchlint"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src -c .eslintrc.json --ext js",
    "format": "prettier {src,__{tests,mocks}__}/**/*.js --write",
    "check_formatted": "prettier **/*.js -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "Waldinar Neto",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "cosmiconfig": "^6.0.0",
    "fs-extra": "^11.3.2",
    "git-branch-is": "^4.0.0",
    "sprintf-js": "^1.1.3",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-jest": "^24.9.0",
    "concat-stream": "^2.0.0",
    "conventional-changelog": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^29.7.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netoht/branchlint.git"
  }
}
